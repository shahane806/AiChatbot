<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
      <handlers>
      <add name="iisnode" path="index.js" verb="*" modules="iisnode" />
    </handlers>
        <directoryBrowse enabled="true" />
        <rewrite>
      <rules>
        <rule name="myapp">
          <match url="/*" />
          <action type="Rewrite" url="index.js" />
        </rule>
      </rules>
    </rewrite>
    </system.webServer>
</configuration>
